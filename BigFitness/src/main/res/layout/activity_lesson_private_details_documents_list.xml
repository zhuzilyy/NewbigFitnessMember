                    android:gravity="right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@id/emergency_contact_tel_img"
                    android:text="-" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="紧急联系人电话"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>





            <!--健身需求-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <TextView
                    android:id="@+id/itness_needs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="健身需求"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />

                <EditText

                    android:id="@+id/text_fitness_needs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/details_note_text"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@id/itness_needs"
                    android:background="@null"
                    android:gravity="top"
                    android:includeFontPadding="false"
                    android:lines="4"
                    android:text=""

                    android:textSize="@dimen/dimen_12sp" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>


            <!--健身履历-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <TextView
                    android:id="@+id/itness_CV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="健身履历"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />

                <EditText
                    android:id="@+id/text_fitness_CV"
