                    android:layout_width="250dp"
                    android:gravity="center_vertical|right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:text="-"
                    android:layout_alignParentBottom="true"
                    android:layout_toStartOf="@+id/interval_time_img" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="单位名称"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>


            <!--单位地址-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <TextView
                    android:id="@+id/address_business"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="单位地址"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />

                <EditText

                    android:id="@+id/text_address_business"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/address_business"
                    android:background="@null"
                    android:gravity="top"
                    android:includeFontPadding="false"
                    android:lines="4"
                    android:text=""
                    android:textSize="@dimen/dimen_12sp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>



            <!--住宅电话-->
            <RelativeLayout
                android:id="@+id/tel_home_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <ImageView
                    android:id="@+id/expire_time_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/editaaa"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"/>

                <EditText
                    android:background="@null"
                    android:textSize="14sp"
                    android:id="@+id/text_tel_home"
                    android:layout_width="250dp"
                    android:gravity="right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@id/expire_time_img"
                    android:text="-" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="住宅电话"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>

            <!--住宅地址-->
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <TextView
                    android:id="@+id/address_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="住宅地址"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />

                <EditText

                    android:id="@+id/text_address_home"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@id/address_home"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@id/address_home"
                    android:background="@null"
                    android:gravity="top"
                    android:includeFontPadding="false"
                    android:lines="4"
                    android:text=""
                    android:textSize="@dimen/dimen_12sp" />

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>



            <!--紧急联系人-->
            <RelativeLayout
                android:id="@+id/emergency_contact_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
