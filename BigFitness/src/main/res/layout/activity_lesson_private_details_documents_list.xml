                    android:layout_marginTop="5dp"
                    android:text="型号"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>

            <!--车牌号-->
            <RelativeLayout
                android:id="@+id/car_id_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <ImageView
                    android:id="@+id/car_id_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/editaaa"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

                <EditText
                    android:background="@null"
                    android:textSize="@dimen/dimen_14sp"
                    android:id="@+id/text_car_id"
                    android:layout_width="250dp"
                    android:gravity="right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@id/car_id_img"
                    android:text="-" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="车牌号"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>




            <!--列表布局-->
            <android.support.v4.widget.SwipeRefreshLayout
                android:id="@+id/swiperefreshlayoutid"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_above="@+id/main_bottom_layout"

                android:layout_below="@+id/toolbar">


                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerviewid"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                </android.support.v7.widget.RecyclerView>
            </android.support.v4.widget.SwipeRefreshLayout>


        </LinearLayout>


    </ScrollView>

</LinearLayout>
