<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_address_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.member.gufei.bigfitness.com.GuFei.Ui.AddressList.AddressDetails.AddressDetailsActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="?attr/colorPrimary"
        android:fitsSystemWindows="true"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:minHeight="?attr/actionBarSize"
        app:titleTextColor="@android:color/white">


        <TextView

            android:id="@+id/tv_title"
            style="@style/title_textview"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dimen_36dp"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/dimen_8dp"
            android:layout_marginTop="@dimen/dimen_8dp"
            android:padding="@dimen/dimen_4dp"
            android:text="会员详细信息"
            android:textAlignment="center"
            android:textColor="@color/all_back_color" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:layout_marginRight="10dp">

            <TextView
                android:id="@+id/btn_complete"
                style="@style/title_textview"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen_36dp"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/dimen_8dp"
                android:layout_marginTop="12dp"
                android:padding="@dimen/dimen_4dp"
                android:text="编辑"
                android:textAlignment="center"
                android:textColor="@color/all_back_color"
               />

        </LinearLayout>

    </android.support.v7.widget.Toolbar>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/lightblack"></LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <!--出生日期-->
            <RelativeLayout
                android:id="@+id/birth_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <ImageView
                    android:id="@+id/birth_type_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/editaaa"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />


                <EditText
                    android:background="@null"
                    android:textSize="14sp"
                    android:id="@+id/text_birth"
                    android:layout_width="250dp"
                    android:gravity="center_vertical|right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/birth_type_img"
                    android:text="-" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="出生日期"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>



            <!--国籍-->
            <RelativeLayout
                android:id="@+id/nationality_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <ImageView
                    android:id="@+id/appointment_type_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/editaaa"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

                <EditText
                    android:background="@null"
                    android:textSize="14sp"
                    android:id="@+id/text_nationality"
                    android:layout_width="250dp"
                    android:gravity="center_vertical|right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@+id/appointment_type_img"
                    android:text="-" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="国    籍"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>

            <!--民族-->
            <RelativeLayout
                android:id="@+id/family_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp">


                <ImageView
                    android:id="@+id/course_type_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/editaaa"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

                <EditText
                    android:background="@null"
                    android:textSize="14sp"
                    android:id="@+id/text_family"
                    android:layout_width="250dp"
                    android:gravity="center_vertical|right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="5dp"
                    android:layout_toLeftOf="@id/course_type_img"
                    android:text="-" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="民    族"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>

            <!--证件类型-->
            <RelativeLayout
                android:id="@+id/id_type_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/begin_time_btn"
                android:gravity="center"
                android:padding="10dp">


                <ImageView
                    android:id="@+id/begin_vailable_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/editaaa"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

                <TextView

                    android:textSize="14sp"
                    android:id="@+id/text_id_type"
                    android:layout_width="250dp"
                    android:gravity="center_vertical|right"
                    android:layout_height="match_parent"

                    android:layout_marginRight="8dp"
                    android:text="-"
                    android:textColor="#C0C0C0"
                    android:layout_alignParentBottom="true"
                    android:layout_toStartOf="@+id/begin_vailable_img" />

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="5dp"
                    android:text="证件类型"
                    android:textColor="#ff333333"
                    android:textSize="14sp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/bg_color"></LinearLayout>


            <!--证件号码-->
            <RelativeLayout
                android:id="@+id/id_no_btn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/begin_time_btn"
                android:gravity="center"
                android:padding="10dp">id_no


                <ImageView
                    android:id="@+id/begin_time_img"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:scaleType="centerInside"
                    android:src="@mipmap/editaaa"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true" />

                <EditText
                    android:background="@null"
                    android:textSize="14sp"
                    android:id="@+id/text_id_no"
                    android:layout_width="250dp"
                    android:gravity="center_vertical|right"
                    android:layout_height="match_parent"
                    android:layout_marginRight="8dp"
